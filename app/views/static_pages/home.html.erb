<h1>Tasks TODO today <%= current_user.first_name %></h1>

<table class="table span12">
	<tr>
		<th>Due Date</th>
		<th>Name</th>
		<th>Priority</th>
	</tr>
	<% Task.tasks_expired(current_user.id).each do |task| %>
	<tr>
		<td><%= task.due_date.strftime("%d/%m/%Y")%> </td>
		<td><%= task.name%> </td>
		<td><%= task.priority%> </td>
	<% end %>
</table>